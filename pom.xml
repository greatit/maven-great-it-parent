  <properties>
    <!-- encoding -->
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

    <!-- maven version -->
    <project.maven.version>[3.8.5,4.0.0)</project.maven.version>

    <!-- java version -->
    <maven.compiler.release>11</maven.compiler.release>

    <!-- CXF WSDL codegenerator -->
    <relativeWsdlRoot>src/main/webapp/WEB-INF/wsdl</relativeWsdlRoot>
    <cxf.wsdlRoot>${project.basedir}/${relativeWsdlRoot}</cxf.wsdlRoot>

    <!-- location -->
    <perftest.report.directory>${project.build.directory}/soapui/report</perftest.report.directory>
    <perftest.log.directory>${project.build.directory}/soapui/log</perftest.log.directory>
    <jenkins.server>https://jenkins.prod.internal.great-it.com</jenkins.server>
    <!-- Jenkins' Github OAuth does not care about user names, only personal access tokens -->
    <jenkins.username>username-irrelevant</jenkins.username>
    <!-- Github personal access token with the following permissions -->
    <!-- * admin:org/read:org -->
    <!-- * user/user:email -->
    <jenkins.password>${env.JENKINSFILE_PLUGIN_GITHUB_PERSONAL_ACCESS_TOKEN}</jenkins.password>
    <nvdApiKey>${env.NVD_API_KEY}</nvdApiKey>

    <!-- dependency -->
    <asciidoctorj-diagram.version>2.0.1</asciidoctorj-diagram.version>
    <asciidoctorj-pdf.version>1.6.2</asciidoctorj-pdf.version>
    <cxf.version>3.6.2</cxf.version>
    <cxf-xjcplugins.version>3.3.0</cxf-xjcplugins.version>
    <dbunit.version>2.7.3</dbunit.version>
    <eclipselink.version>4.0.0</eclipselink.version>
    <el-api.version>3.0.0</el-api.version>
    <git-commit-id-plugin.version>4.9.10</git-commit-id-plugin.version>
    <groovy.version>3.0.14</groovy.version>
    <guava.version>32.1.2-jre</guava.version>
    <hibernate.version>5.6.2.Final</hibernate.version>
    <!-- 6.x required due to https://stackoverflow.com/a/74932171/2728862 -->
    <hibernate-validator.version>8.0.1.Final</hibernate-validator.version>
    <jackson.version>2.15.3</jackson.version>
    <jakarta-expression.version>4.0.2</jakarta-expression.version>
    <jakarta-persistence.version>3.1.0</jakarta-persistence.version>
    <jakarta-validation.version>3.0.2</jakarta-validation.version>
    <log4j.version>2.22.0</log4j.version>
    <lombok.version>1.18.30</lombok.version>
    <modelmapper.version>3.1.0</modelmapper.version>
    <mysql.version>8.0.22</mysql.version>
    <persistence.version>2.2.1</persistence.version>
    <slf4j.version>1.7.29</slf4j.version>
    <!--
      must override Spring Boot's SnakeYAML dependency version
      since the most  recent version 2.7.5 still contains vulnerable
      SnakeYAML 1.30 on 23.11.2022
     -->
    <snakeyaml.version>1.33</snakeyaml.version>
    <soapui.version>5.7.0</soapui.version>
    <spotbugs.version>4.7.3</spotbugs.version>
    <spring-boot.version>2.7.17</spring-boot.version>
    <spring-test-dbunit.version>1.3.0</spring-test-dbunit.version>
    <testcontainers.version>1.17.6</testcontainers.version>

    <!-- plugin -->
    <asciidoctor-plugin.version>2.2.2</asciidoctor-plugin.version>
    <build-helper-plugin.version>3.3.0</build-helper-plugin.version>
    <changes-plugin.version>2.12.1</changes-plugin.version>
    <checkstyle-plugin.version>3.1.2</checkstyle-plugin.version>
    <clover-plugin.version>4.4.1</clover-plugin.version>
    <cxf-plugin.version>${cxf.version}</cxf-plugin.version>
    <dbunit-plugin.version>1.0-beta-3</dbunit-plugin.version>
    <dependency-check-plugin.version>8.4.3</dependency-check-plugin.version>
    <exec-plugin.version>3.1.0</exec-plugin.version>
    <findsecbugs-plugin.version>1.12.0</findsecbugs-plugin.version>
    <formatter-plugin.version>2.18.0</formatter-plugin.version>
    <hibernate-tools-plugin.version>${hibernate.version}</hibernate-tools-plugin.version>
    <lifecycle-mapping-plugin.version>1.0.0</lifecycle-mapping-plugin.version>
    <liquibase-plugin.version>4.17.2</liquibase-plugin.version>
    <pmd-plugin.version>3.19.0</pmd-plugin.version>
    <processor-plugin.version>4.5</processor-plugin.version>
    <spotbugs-plugin.version>4.7.3.0</spotbugs-plugin.version>
    <spring-boot-plugin.version>${spring-boot.version}</spring-boot-plugin.version>
    <staticweave-plugin.version>1.0.0</staticweave-plugin.version>
    <tidy-plugin.version>1.1.0</tidy-plugin.version>
    <versions-plugin.version>2.10.0</versions-plugin.version>
    <jenkinsfile-plugin.version>1.0.1</jenkinsfile-plugin.version>

    <!-- extension -->
    <wagon-ssh-extension.version>3.3.2</wagon-ssh-extension.version>
  </properties>

  <dependencyManagement>
